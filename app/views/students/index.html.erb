<!--<h1>Tracker</h1>-->


<% @students.each do |student| %>
<div class="status">
  <p><strong> <%= student.firstname %>  </strong></p>
  <p>  <font style="background-color:#99CCCC"><i class="fa fa-meh-o"></i> <%= student.behive %></font> | <i><font style="background-color:#FFCCCC"><i class="fa fa-check-square"></i> <%= student.intervention_type %></i></font> </p>
  <div class="meta">
  <%= time_ago_in_words(student.created_at) %>
  <span class="admin">
  <%= link_to 'View', student %>
  <%= link_to 'Edit', edit_student_path(student) %>
  <%= link_to 'Delete', student, method: :delete, data: { confirm: 'Are you sure?' } %>
</span>
</div>
</div>
  <% end %>


